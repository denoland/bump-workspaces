{
  "lock": false,
  "name": "@deno/bump-workspaces",
  "exports": {
    ".": "./mod.ts",
    "./cli": "./cli.ts"
  },
  "version": "0.1.23",
  "tasks": {
    "test": "deno test -A",
    "cov": "rm -rf coverage && deno test -A --coverage && deno coverage --html && deno coverage --lcov --output=coverage/lcov.info",
    "cov-open": "deno task cov && open coverage/html/index.html"
  },
  "imports": {
    "@david/dax": "jsr:@david/dax@^0.40.1",
    "@std/assert": "jsr:@std/assert@^0.224.0",
    "@std/cli": "jsr:@std/cli@^0.224.0",
    "@std/fmt": "jsr:@std/fmt@^0.224.0",
    "@std/fs": "jsr:@std/fs@^0.224.0",
    "@std/jsonc": "jsr:@std/jsonc@^0.224.0",
    "@std/path": "jsr:@std/path@^0.224.0",
    "@std/semver": "jsr:@std/semver@^0.224.0",
    "@std/testing": "jsr:@std/testing@^0.224.0"
  }
}
